            <string>source.plantuml.plantumlActivitySynch</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\%(^\%(#\S\+\)\?\)\@&lt;=:\_[^;|&lt;&gt;/\]}]\+[;|&lt;&gt;/\]}]$/ contains=plantumlSpecialString</string>
            <key>name</key>
            <string>source.plantuml.plantumlActivityLabel</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*==[^=]\+==\s*$/</string>
            <key>name</key>
            <string>source.plantuml.plantumlSequenceDivider</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*|||\+\s*$/</string>
            <key>name</key>
            <string>source.plantuml.plantumlSequenceSpace</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*||\d\+||\+\s*$/</string>
            <key>name</key>
            <string>source.plantuml.plantumlSequenceSpace</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*\.\{3}$/</string>
            <key>name</key>
            <string>source.plantuml.plantumlSequenceDelay</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*:.\{-1,}:/ contains=plantumlSpecialString</string>
            <key>name</key>
            <string>source.plantuml.plantumlUsecaseActor</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\[[^\]]\{-}\]\%('s\)\?/ contains=plantumlSpecialString</string>
            <key>name</key>
            <string>source.plantuml.plantumlGanttTask</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/%\%(chr\|darken\|date\|dec2hex\|dirpath\|feature\|false\|file_exists\|filename\|function_exists\|get_variable_value\|getenv\|hex2dec\|hsl_color\|intval\|is_dark\|is_light\|lighten\|loadJSON\|lower\|newline\|not\|lighten\|reverse_color\|reverse_hsluv_color\|set_variable_value\|size\|string\|strlen\|strpos\|substr\|true\|upper\|variable_exists\|version\)/</string>
            <key>name</key>
            <string>source.plantuml.plantumlBuiltinFunction</string>
