<?xml version="1.0" ?>
<plist version="1.0">
   <dict>
      <key>name</key>
      <string>PlantUML</string>
      <key>scopeName</key>
      <string>source.plantuml</string>
      <key>patterns</key>
      <array>
         <dict>
            <key>match</key>
            <string>/\%(^@start\|^@end\)\%(board\|bpm\|creole\|cute\|def\|ditaa\|dot\|flow\|gantt\|git\|jcckit\|json\|latex\|math\|mindmap\|nwdiag\|project\|salt\|tree\|uml\|wbs\|wire\|yaml\)/</string>
            <key>name</key>
            <string>source.plantuml.plantumlPreProc</string>
