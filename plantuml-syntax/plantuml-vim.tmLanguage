            <string>source.plantuml.plantumlSpecialString</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*'.*$/ contains=plantumlCommentTODO</string>
            <key>name</key>
            <string>source.plantuml.plantumlComment</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/&lt;\/\?[bi]&gt;/</string>
