            <key>name</key>
            <string>source.plantuml.plantumlArrowDirectedLine</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\%(^\s*[rh]\?\%(note\|legend\)\)\@&lt;=\s\%([^:]\+$\)/ contained contains=plantumlKeyword,plantumlColor,plantumlString,plantumlTag</string>
            <key>name</key>
            <string>source.plantuml.plantumlNoteMultiLineStart</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*+\s*\w\+/ contained</string>
            <key>name</key>
            <string>source.plantuml.plantumlClassPublic</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*-\s*\w\+/ contained</string>
            <key>name</key>
            <string>source.plantuml.plantumlClassPrivate</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/^\s*#\s*\w\+/ contained</string>
