            <string>source.plantuml.plantumlColonLine</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/&lt;&lt;[^-.]\+&gt;&gt;/ contains=plantumlSpecialString</string>
            <key>name</key>
            <string>source.plantuml.plantumlStereotype</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/([^)]*)/</string>
            <key>name</key>
            <string>source.plantuml.plantumlActivityThing</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/===[^=]\+===/</string>
