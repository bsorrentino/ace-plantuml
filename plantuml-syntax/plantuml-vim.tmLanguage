            <key>match</key>
            <string>/\([-.]\)\1*\%(\w\{,5\}\1\+\)\?\%(|&gt;\|&gt;\|*\|o\|x\|#\|{\|+\|\^\)/ contains=plantumlArrowDirectedLine</string>
            <key>name</key>
            <string>source.plantuml.plantumlClassRelationLR</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\%(&lt;|\|&lt;\|*\|o\|x\|#\|}\|+\|\^\)\([-.]\)\1*\%(\w\{,5\}\1\+\)\?/ contains=plantumlArrowDirectedLine</string>
            <key>name</key>
            <string>source.plantuml.plantumlClassRelationRL</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\[\?\([-.]\)\1*\%(\w\{,5}\1\+\)\?\%(\[[^\]]\+\]\)\?\1*\(&gt;\|\\\|\/\)\2\?[ox]\?\]\?\%(\[[^\]]*\]\)\?/ contains=plantumlText,plantumlArrowDirectedLine</string>
            <key>name</key>
            <string>source.plantuml.plantumlArrowLR</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/\[\?[ox]\?\(&lt;\|\\\|\/\)\1\?\([-.]\)\2*\%(\w\{,5}\2\+\)\?\]\?\%(\[[^\]]*\]\)\?/ contains=plantumlText,plantumlArrowDirectedLine</string>
            <key>name</key>
            <string>source.plantuml.plantumlArrowRL</string>
         </dict>
         <dict>
            <key>match</key>
            <string>/[ox]\?\(&lt;\|\\\|\/\)\1\?\([-.]\)\2*\%(\w\{,5}\2\+\)\?\(&gt;\|\\\|\/\)\3\?[ox]\?/ contains=plantumlArrowDirectedLine</string>
            <key>name</key>
            <string>source.plantuml.plantumlArrowBoth</string>
         </dict>
         <dict>
